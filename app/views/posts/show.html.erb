<h1><%= @post.title %></h1>
<% if @post.link %>
    <%= image_tag(@post.link) %><br>
<% end %>
<% if @post.images.attached?%>
    <% @post.images.each do |image| %>
        <%= image_tag(image) %>
    <% end %>
<% end %>
<br>
<h4><%= @post.content %></h4><br>
<h4><%= image_tag('ThumbsUp.png', width: '20px', height: '20px')%><%= @post.likes %></h4><br>
